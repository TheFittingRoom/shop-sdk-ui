{
  "name": "@thefittingroom/shop-ui",
  "version": "3.0.0-alpha-16",
  "description": "the fitting room UI library",
  "type": "module",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist && rm -rf build && rm -rf .rollup.cache",
    "build:prod": "npm run clean && cross-env NODE_ENV=prod rollup --config",
    "build:dev": "npm run clean && cross-env NODE_ENV=dev rollup --config",
    "watch:prod": "npm run clean && cross-env NODE_ENV=prod rollup --config -w",
    "watch:dev": "npm run clean && cross-env NODE_ENV=dev rollup --config -w",
    "serve": "http-server --cors -p 3030 -a localhost",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "prettier --write 'src/**/*.{js,ts,css}'",
    "pub": "npm run build:prod && npm publish --access public"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "http-server": "^14.1.1",
    "prettier": "^3.1.0",
    "rollup": "^2.79.1",
    "rollup-plugin-dotenv": "^0.4.0",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@thefittingroom/sdk": "3.0.0-alpha-13"
  },
  "resolutions": {
    "undici": "^5.28.6"
  },
  "overrides": {
    "undici": "^5.28.6"
  }
}
